    <dependency>
      <groupId>com.github.jsqlparser</groupId>
      <artifactId>jsqlparser</artifactId>
      <version>5.4-SNAPSHOT</version>
    </dependency>
    <dependency>
      <groupId>ai.starlake.jsqltranspiler</groupId>
      <artifactId>jsqltranspiler</artifactId>
      <version>1.0</version>
    </dependency>
  </dependencies>
</project>
